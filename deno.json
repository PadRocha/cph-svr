{
  "tasks": {
    "start": "deno run --allow-read --allow-write --allow-net --allow-env --allow-sys --allow-ffi --allow-run main.ts",
    "dev": "deno run --allow-read --allow-write --allow-net --allow-env --allow-sys --allow-ffi --allow-run --watch main.ts"
  },
  "imports": {
    "@hono/hono": "jsr:@hono/hono@^4.6.13",
    "@std/assert": "jsr:@std/assert@1",
    "@std/crypto": "jsr:@std/crypto@^1.0.3",
    "@std/dotenv": "jsr:@std/dotenv@^0.225.3",
    "@std/fs": "jsr:@std/fs@^1.0.6",
    "@std/path": "jsr:@std/path@^1.0.8",
    "jose": "npm:jose@^6.0.8",
    "mongoose": "npm:mongoose@^8.9.5",
    "sharp": "npm:sharp@^0.33.5",

    "deps": "./src/deps.ts",
    "factory": "./src/factory.ts",
    "config": "./src/config/config.ts",
    "aggregate": "./src/config/aggregate.ts",
    "regex": "./src/config/regex.ts",
    "errors": "./src/errors/index.ts",
    "middlewares": "./src/middlewares/index.ts",
    "models": "./src/models/index.ts",
    "controllers": "./src/controllers/index.ts",
    "routers": "./src/routers/index.ts",

    "@utils/": "./src/utils/",
    "@services/": "./src/services/",
    "@interfaces/": "./src/interfaces/"
  },
  "nodeModulesDir": "auto"
}
